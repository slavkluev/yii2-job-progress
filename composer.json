{
    "name": "slavkluev/yii2-job-progress",
    "type": "library",
    "description": "",
    "keywords": [
        "slavkluev",
        "yii2-job-progress"
    ],
    "homepage": "https://github.com/slavkluev/yii2-job-progress",
    "license": "MIT",
    "authors": [
        {
            "name": "Viacheslav Kliuev",
            "email": "slavkluev@yandex.ru",
            "homepage": "https://github.com/slavkluev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.2",
        "yiisoft/yii2-queue": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "slavkluev\\JobProgress\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "slavkluev\\JobProgress\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
